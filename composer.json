{
    "name": "fsi/datasource-bundle",
    "type": "symfony-bundle",
    "description": "FSi DataSource Bundle",
    "keywords": ["fsi", "symfony", "datasource", "datasource-bundle", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lukasz Cybula",
            "email": "lukasz@fsi.pl"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "twig/twig": "~1.12",
        "symfony/framework-bundle" : "^2.2|^3.0",
        "symfony/options-resolver": "^2.6|^3.0",
        "symfony/dependency-injection" : "^2.2|^3.0",
        "symfony/doctrine-bridge": "^2.2|^3.0",
        "symfony/security-csrf": "^2.4|^3.0",
        "symfony/yaml": "^2.2|^3.0",
        "symfony/translation": "^2.2|^3.0",
        "fsi/datasource": "~1.2@dev",
        "doctrine/orm": "~2.3",
        "doctrine/collections": "~1.0"
    },
    "require-dev": {
        "symfony/form" : "^2.2|^3.0",
        "symfony/twig-bridge" : "^2.2|^3.0",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": { "FSi\\Bundle\\DataSourceBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    }
}
